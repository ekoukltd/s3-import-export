{
    "name": "ekoukltd/s3-import-export",
    "description": "Exports & Imports Tables to Local or S3 Storage",
    "keywords": [
        "ekoukltd",
        "s3-import-export",
        "export-laravel-models-to-s3"
    ],
    "homepage": "https://github.com/ekoukltd/s3-import-export",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lee Evans",
            "email": "support@ekouk.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8 || ^9"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ekoukltd\\S3ImportExport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ekoukltd\\S3ImportExport\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ekoukltd\\S3ImportExport\\S3IOServiceProvider"
            ],
            "aliases": {
                "S3ImportExport": "S3IOFacade"
            }
        }
    }
}
